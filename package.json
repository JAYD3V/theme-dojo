{
  "name": "github-vscode-theme",
  "displayName": "GitHub Theme",
  "description": "GitHub theme for VS Code",
  "version": "6.3.4",
  "publisher": "GitHub",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1b1f23",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JAYD3V/theme-dojo"
  },
  "bugs": {
    "url": "https://github.com/JAYD3V/theme-dojo/issues"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "light",
    "dark",
    "protanopia",
    "colorblind"
  ],
  "contributes": {
    "themes": [
      {
        "label": "GitHub Light Default",
        "uiTheme": "vs",
        "path": "./themes/light-default.json"
      }
    ]
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.17.0",
    "@primer/primitives": "7.10.0",
    "chroma-js": "^2.1.2",
    "color": "^3.1.2",
    "nodemon": "^2.0.3",
    "vsce": "^1.100.1"
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/main.js",
    "package": "vsce package -o ./build/github-vscode-theme.vsix",
    "prepublishOnly": "npm run build && vsce publish",
    "release": "changeset publish"
  },
  "publishConfig": {
    "access": "public"
  }
}
